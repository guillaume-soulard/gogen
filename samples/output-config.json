{
  "template": {
    "id": {
      "_type": "number",
      "options": {
        "bounds": {
          "min": 1,
          "max": 9999999999999999
        },
        "sequence": {
          "enable": true,
          "cycle": false
        }
      }
    },
    "firstName": {
      "_type": "string",
      "options": {
        "pattern": "[A-Z]{1}[a-z]{5,25}"
      }
    },
    "lastName": {
      "_type": "string",
      "options": {
        "pattern": "[A-Z]{5,25}"
      }
    },
    "dayOfBirth": {
      "_type": "date",
      "options": {
        "bounds": {
          "min": "1940-01-01T00:00:00",
          "max": "2015-12-31T23:59:59"
        },
        "truncate": "hours"
      }
    }
  },
  "outputs": [
    {
      "type": "stdout",
      "format": {
        "type": "json",
        "options": {
          "pretty": false
        }
      }
    }
  ]
}